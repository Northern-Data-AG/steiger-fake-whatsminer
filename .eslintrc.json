{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["xo", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "dot-notation": "off",
    "no-bitwise": "off",
    "max-depth": "off",
    "no-negated-condition": "off",
    "new-cap": "off"
  }
}
